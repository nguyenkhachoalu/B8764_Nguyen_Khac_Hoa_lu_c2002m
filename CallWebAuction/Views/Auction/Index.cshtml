@model IEnumerable<CallWebAuction.Models.Auction>
@{
    ViewData["AdminView"] = "Index";
}
    <!-- ============================================================== -->
    <div class="content-page">
        <!-- Start content -->
        <div class="content">
            <div class="container-fluid">
                <div class="page-title-box">

                    <div class="panel panel-primary">
                        <div class="panel-heading">User</div>
                        <div class="panel-body">
                            <table class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Id</th>
                                        <th>Name</th>
                                        <th>Top</th>
                                        <th>Price</th>
                                        <th>Status</th>
                                        <th>StartedDate</th>
                                        <th>EndData</th>
                                        <th>CreatedDate</th>
                                        <th></th>
                                    </tr>
                                </thead>

                                @foreach (var item in Model)
                                {
                                    <tbody>
                                        <tr>
                                            <td>
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Id)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Id_Product)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Top_User)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Top_Price)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Status)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.StartedDate)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.EndTime)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.CreatTime)
                                            </td>
                                            <td>
                                                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |

                                                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                                            </td>
                                        </tr>
                                    </tbody>

                                }
                            </table>
                        </div>
                    </div>

                </div>

            </div>
            <!-- END ROW -->

        </div>
        <!-- container-fluid -->

    </div>
    <!-- content -->

<!-- ============================================================== -->
<!-- End Right content here -->
<!-- ============================================================== -->
<!-- END wrapper -->
